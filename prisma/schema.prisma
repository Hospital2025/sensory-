datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  service   String
  date      DateTime
  time      String
  status    String   @default("pending")
  createdAt DateTime @default(now())
}
